{
  "name": "simple-web-angular-resume",
  "version": "0.0.0",
  "author": "Jean-Philippe Maquestiaux",
  "description": "A personal resume website built with Angular and TypeScript",
  "keywords": [
    "resume",
    "portfolio",
    "angular",
    "typescript",
    "tailwindcss"
  ],
  "license": "MIT",
  "packageManager": "npm@10.9.2",
